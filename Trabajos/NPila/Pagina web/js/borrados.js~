function borrarTurno(id)
	{
 var id = $(this).val();
 $(function()
        {
            $.ajax({
			type: "POST",
			url: "turnos.php",
			data:$("#id_turno").serialize(),
			success: function(data)
				{
					document.getElementById('content').innerHTML=id;
				}
		});
		return false;
	});
}

